{
  "name": "testra",
  "version": "0.1",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "yarn run env && ng build --prod --aot --build-optimizer --vendor-chunk=true",
    "start": "yarn run env && ng serve --aot --proxy-config proxy.conf.js",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "yarn run env && ng test",
    "test:ci": "yarn run env && yarn run lint && ng test --code-coverage --watch=false",
    "e2e": "yarn run env && ng e2e",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^6.1.7",
    "@angular/common": "^6.1.7",
    "@angular/compiler": "^6.1.7",
    "@angular/core": "^6.1.7",
    "@angular/forms": "^6.1.7",
    "@angular/http": "^6.1.7",
    "@angular/platform-browser": "^6.1.7",
    "@angular/platform-browser-dynamic": "^6.1.7",
    "@angular/router": "^6.1.7",
    "@fortawesome/angular-fontawesome": "^0.2.1",
    "@fortawesome/fontawesome-free": "^5.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@nebular/theme": "^2.0.0-rc.10",
    "@ng-bootstrap/ng-bootstrap": "^3.2.2",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/entity": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "angular-split-ng6": "^1.0.0-rc.5",
    "angulartics2": "^6.3.0",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.6",
    "echarts": "^4.1.0",
    "lodash": "^4.17.11",
    "ngx-echarts": "^4.0.0",
    "ngx-highlightjs": "^2.1.1",
    "numeral": "^2.0.6",
    "pretty-ms": "^4.0.0",
    "rxjs": "^6.3.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.2",
    "@angular/cli": "~6.2.2",
    "@angular/compiler-cli": "^6.1.7",
    "@angular/language-service": "^6.1.7",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
